{
  "name": "yourprime-v2",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Un creador y seguidor de rutinas de gimnasio inteligente.",
  "scripts": {
    "postinstall": "chmod +x node_modules/.bin/esbuild",
    "build": "npx esbuild index.tsx --bundle --outfile=www/index.js --sourcemap --jsx=automatic --loader:.js=jsx --loader:.ts=tsx --format=esm && npx cpx2 index.html www && npx cpx2 favicon.svg www && npx cpx2 manifest.json www && npx cpx2 service-worker.js www && npx cpx2 icon-192.png www && npx cpx2 icon-512.png www && npx cpx2 widget-template.html www",
    "serve": "npx serve www -l 5500",
    "dev": "npm run build && npm run serve",
    "cap:add": "npx cap add android",
    "cap:sync": "npm run build && npx cap sync android",
    "cap:open": "npx cap open android"
  },
  "dependencies": {
    "@capacitor-community/keep-awake": "^6.0.0",
    "@capacitor-community/native-audio": "6.0.0",
    "@capacitor-community/text-to-speech": "6.0.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/geolocation": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/local-notifications": "^6.0.0",
    "@capacitor/preferences": "^6.0.0",
    "@capacitor/share": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@google/genai": "^1.16.0",
    "html2canvas": "^1.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.12.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/cli": "^6.0.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "cpx2": "^4.2.0",
    "esbuild": "^0.20.2",
    "typescript": "^5.5.2"
  },
  "overrides": {
    "@capacitor-community/keep-awake": {
      "@capacitor/core": "^6.0.0"
    },
    "@capacitor-community/text-to-speech": {
      "@capacitor/core": "^6.0.0"
    }
  }
}
